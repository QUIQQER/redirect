<?xml version="1.0" encoding="UTF-8"?>
<database>
    <global>
        <table name="redirects">
            <field type="VARCHAR( 32 ) NOT NULL">id</field>
            <field type="VARCHAR( 2083 ) NOT NULL">source_url</field>
