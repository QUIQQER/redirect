<?xml version="1.0" encoding="UTF-8"?>
<locales>
    <groups name="quiqqer/redirect" datatype="php,js">
        <locale name="package.title">
            <de><![CDATA[Weiterleitung]]></de>
            <en><![CDATA[Redirect]]></en>
        </locale>
        <locale name="package.description">
            <de><![CDATA[Automatische Weiterleitung gelöschter/verschobener Seiten]]></de>
            <en><![CDATA[Automatic redirect of moved/deleted sites]]></en>
        </locale>

        <!-- region site move -->
        <locale name="site.move.info">
            <de><![CDATA[Umleitung für diese und alle Unterseiten hinzugefügt.]]></de>
            <en><![CDATA[Redirect was added for this and all child-sites.]]></en>
        </locale>

        <locale name="site.move.error">
            <de><![CDATA[Umleitung anlegen für eine Seite nicht möglich.]]></de>
            <en><![CDATA[Failed adding redirect for a site.]]></en>
        </locale>
        <!-- endregion -->
    </groups>

    <groups name="quiqqer/redirect" datatype="js">
        <!-- region delete popup -->
        <locale name="site.delete.popup.title">
            <de><![CDATA[Add redirect for deleted site]]></de>
            <en><![CDATA[Umleitung für gelöschte Seite hinzufügen]]></en>
        </locale>
        <locale name="site.delete.popup.button.ok.text">
            <de><![CDATA[Hinzufügen]]></de>
            <en><![CDATA[Add]]></en>
        </locale>

        <!-- region errors -->
        <locale name="site.delete.popup.error.select.none">
            <de><![CDATA[Es muss eine Seite ausgewählt werden.]]></de>
            <en><![CDATA[You have to select a page.]]></en>
        </locale>
        <locale name="site.delete.popup.error.result">
            <de><![CDATA[Die Umleitung konnte nicht hinzugefügt werden.]]></de>
            <en><![CDATA[The redirect could not be added.]]></en>
        </locale>
        <!-- endregion -->
        <!-- endregion -->
    </groups>
</locales>
