<?xml version="1.0" encoding="UTF-8"?>
<locales>
    <groups name="quiqqer/redirect" datatype="php,js">
        <locale name="package.title">
            <de><![CDATA[Weiterleitung]]></de>
            <en><![CDATA[Redirect]]></en>
        </locale>
        <locale name="package.description">
            <de><![CDATA[Automatische Weiterleitung gelöschter/verschobener Seiten]]></de>
            <en><![CDATA[Automatic redirect of moved/deleted sites]]></en>
        </locale>

        <locale name="menu.text">
            <de><![CDATA[Umleitungen]]></de>
            <en><![CDATA[Redirects]]></en>
        </locale>

        <!-- region site move -->
        <locale name="site.move.info">
            <de><![CDATA[Umleitung für diese und alle Unterseiten hinzugefügt.]]></de>
            <en><![CDATA[Redirect was added for this and all child-sites.]]></en>
        </locale>

        <locale name="site.move.error">
            <de><![CDATA[Umleitung anlegen für eine Seite nicht möglich.]]></de>
            <en><![CDATA[Failed adding redirect for a site.]]></en>
        </locale>
        <!-- endregion -->
    </groups>

    <groups name="quiqqer/redirect" datatype="js">
        <!-- region add redirect popup -->
        <locale name="window.redirect.title">
            <de><![CDATA[Umleitung hinzufügen]]></de>
            <en><![CDATA[Add redirect]]></en>
        </locale>
        <locale name="site.delete.popup.button.ok.text">
            <de><![CDATA[Umleitung hinzufügen]]></de>
            <en><![CDATA[Add redirect]]></en>
        </locale>
        <locale name="site.delete.popup.button.cancel.text">
            <de><![CDATA[Nicht hinzufügen]]></de>
            <en><![CDATA[Don't add]]></en>
        </locale>



        <locale name="window.redirect.children.skip">
            <de><![CDATA[Ziel-URL für alle weiteren Seiten übernehmen]]></de>
            <en><![CDATA[Apply target-URL to all further sites]]></en>
        </locale>

        <locale name="window.redirect.url.source">
            <de><![CDATA[Quell-URL]]></de>
            <en><![CDATA[Source-URL]]></en>
        </locale>

        <locale name="window.redirect.url.target">
            <de><![CDATA[Ziel-URL]]></de>
            <en><![CDATA[Target-URL]]></en>
        </locale>

        <!-- region panel -->
        <locale name="panel.title">
            <de><![CDATA[Umleitungen]]></de>
            <en><![CDATA[Redirects]]></en>
        </locale>

        <locale name="panel.button.redirect.add">
            <de><![CDATA[Umleitung Hinzufügen]]></de>
            <en><![CDATA[Add Redirect]]></en>
        </locale>

        <locale name="panel.button.redirect.delete">
            <de><![CDATA[Umleitung löschen]]></de>
            <en><![CDATA[Delete Redirect]]></en>
        </locale>

        <locale name="panel.button.redirect.edit">
            <de><![CDATA[Umleitung bearbeiten]]></de>
            <en><![CDATA[Edit Redirect]]></en>
        </locale>
        <!-- endregion -->


        <!-- region errors -->
        <locale name="window.redirect.url.target.error">
            <de><![CDATA[Es muss eine Ziel-URL ausgewählt werden.]]></de>
            <en><![CDATA[You have to select a target-URL.]]></en>
        </locale>

        <locale name="window.redirect.url.target.note">
            <de><![CDATA[URLs, die mit "index.php?" beginnen, werden zu ihren sprechenden URLs umgeschrieben.]]></de>
            <en><![CDATA[URLs, starting with "index.php?", will be rewritten to their proper URLs.]]></en>
        </locale>

        <locale name="site.delete.popup.error.result">
            <de><![CDATA[Die Umleitung konnte nicht hinzugefügt werden.]]></de>
            <en><![CDATA[The redirect could not be added.]]></en>
        </locale>
        <!-- endregion -->
        <!-- endregion -->

        <!-- region delete redirect -->
        <locale name="window.redirect.delete.title">
            <de><![CDATA[Umleitung löschen]]></de>
            <en><![CDATA[Delete redirect]]></en>
        </locale>

        <locale name="window.redirect.delete.text">
            <de><![CDATA[Möchten Sie die Umleitungen für die URLs "[urls]" wirklich löschen?]]></de>
            <en><![CDATA[Do you really want to delete the redirects for the urls "[urls]"?]]></en>
        </locale>

        <locale name="window.redirect.delete.button.ok.text">
            <de><![CDATA[Löschen]]></de>
            <en><![CDATA[Delete]]></en>
        </locale>
        <!-- endregion -->
    </groups>
</locales>
